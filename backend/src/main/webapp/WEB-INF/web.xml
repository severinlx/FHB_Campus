<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
    <servlet>
        <servlet-name>SystemServiceServlet</servlet-name>
        <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
        <init-param>
            <param-name>services</param-name>
            <param-value>org.alex.stundenplan.backend.MyEndpoint, org.alex.stundenplan.backend.ParserEndpoint, org.alex.stundenplan.backend.CombinationEndPoint </param-value>

        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>SystemServiceServlet</servlet-name>
        <url-pattern>/_ah/spi/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CronParse</servlet-name>
        <servlet-class>org.alex.stundenplan.backend.cron.CronParser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CronParse</servlet-name>
        <url-pattern>/cron/cronParser</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CronParseMainUrl</servlet-name>
        <servlet-class>org.alex.stundenplan.backend.cron.urlsUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CronParseMainUrl</servlet-name>
        <url-pattern>/cron/urlsUpdate</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>